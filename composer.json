{
    "name": "the-coding-owl/owl-dav",
    "description": "An extension for TYPO3 that enables DAV functionality using sabre dav",
    "type": "typo3-cms-extension",
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^12.4",
        "typo3/cms-fluid": "^12.4",
        "typo3/cms-extbase": "^12.4",
        "typo3/cms-backend": "^12.4",
        "sabre/dav": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "TheCodingOwl\\OwlDav\\": "Classes/"
        }
    },
    "authors": [
        {
            "name": "Kevin Ditscheid",
            "email": "kevin@the-coding-owl.de"
        }
    ],
    "extra": {
        "typo3/cms": {
            "extension-key": "owl_dav"
        }
    }
}
